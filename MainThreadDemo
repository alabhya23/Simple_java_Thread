public class MainThreadDemo {
    public static void main(String[] args) {
        // Creating thread objects
        Thread numbersThread = new PrintNumbers();
        Thread charactersThread = new PrintCharacters();
        Thread messageThread = new PrintMessage("Learning Java Threads!");

        // Starting threads
        numbersThread.start();
        charactersThread.start();
        messageThread.start();
    }
}
